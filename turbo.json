{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "studio#build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_GIT_COMMIT_HASH",
        "NEXT_PUBLIC_BUILD_TIME",

        "VELA_PLATFORM_STORAGE_SIGNATURE_PRIVATE_KEY",
        "VELA_PLATFORM_STORAGE_ENCRYPTION_PRIVATE_KEY",

        "ANALYZE",
        "NEXT_PUBLIC_SUPPORT_API_URL",
        "NEXT_PUBLIC_CONTENT_API_URL",
        "NEXT_PUBLIC_BASE_PATH",
        "NEXT_PUBLIC_SUPPORT_ANON_KEY",
        "NEXT_PUBLIC_ENVIRONMENT",
        "NEXT_PUBLIC_SITE_URL",
        "NEXT_PUBLIC_API_URL",
        "NEXT_PUBLIC_HCAPTCHA_SITE_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_NODE_ENV",
        "NODE_ENV",
        "SUPABASE_URL",
        // These envs are technically passthrough env vars because they're only used on the server side of Nextjs
        "PLATFORM_PG_META_URL",
        "READ_ONLY_URL",
        "READ_ONLY_API_KEY",
        "SUPABASE_SERVICE_KEY",
        "SUPABASE_ANON_KEY",
        "OPENAI_API_KEY",
        "AUTH_JWT_SECRET",
        "LOGFLARE_URL",
        "LOGFLARE_PRIVATE_ACCESS_TOKEN",
        "FORCE_ASSET_CDN",
        "ASSET_CDN_S3_ENDPOINT",
        "SITE_NAME",
        "VELA_PLATFORM_URL",
        "VELA_PLATFORM_GOTRUE_URL",
        "VELA_PLATFORM_KEYCLOAK_ISSUER",
        "VELA_PLATFORM_KEYCLOAK_CLIENT_ID",
        "VELA_PLATFORM_KEYCLOAK_CLIENT_SECRET",
        "VELA_PLATFORM_KEYCLOAK_AUTHORIZATION_ENDPOINT",
        "VELA_PLATFORM_KEYCLOAK_TOKEN_ENDPOINT",
        "VELA_PLATFORM_KEYCLOAK_USERINFO_ENDPOINT",
        "VELA_PLATFORM_KEYCLOAK_LOGOUT_ENDPOINT",
        "VELA_PLATFORM_KEYCLOAK_JWKS_ENDPOINT",
        "VELA_PLATFORM_EXT_SIGN_IN_URL",
        "VELA_PLATFORM_EXT_BASE_URL",
        "VELA_PLATFORM_EXT_API_SERVICE_URL"
      ],
      "passThroughEnv": ["CURRENT_CLI_VERSION", "VERCEL_GIT_COMMIT_SHA"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "ui-library#build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_BASE_PATH",
        "NEXT_PUBLIC_APP_URL",
        "NEXT_PUBLIC_API_URL",
        "NODE_ENV",
        // These envs are used in the packages
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ],
      "outputs": [".next/**", "!.next/cache/**", ".contentlayer/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    }
  }
}
